{
  "name": "@yngvenot-platform/frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run build:wasm && next dev",
    "build:wasm": "cd ../wasm-app && wasm-pack build --target web --out-dir ../frontend/src/wasm && cd ../frontend",
    "build": "if [ \"$VERCEL\" != \"1\" ]; then npm run build:wasm; fi && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "next": "^14.0.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
